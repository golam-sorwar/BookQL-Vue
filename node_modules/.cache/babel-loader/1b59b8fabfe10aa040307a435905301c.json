ry,\n      selectedCategory: \"all\",\n      query: booksQuery,\n      categories: []\n    };\n  },\n  methods: {\n    selectCategory: function selectCategory(category) {\n      if (category === \"all\") {\n        this.query = booksQuery;\n      } else if (category === \"featured\") {\n        this.query = booksFeaturedQuery;\n      } else {\n        this.query = categoryQuery;\n      }\n\n      this.selectedCategory = category;\n    }\n  }\n};",{"version":3,"sources":["Home.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8FA;AACA,OAAA,aAAA;AACA,OAAA,eAAA;AACA,OAAA,UAAA;AACA,OAAA,kBAAA,6C,CACA;;AAEA,eAAA;AACA,EAAA,IAAA,EAAA,MADA;AAEA,EAAA,UAAA,EAAA,EAFA;AAGA,EAAA,IAHA,kBAGA;AACA,WAAA;AACA,MAAA,aAAA,EAAA,aADA;AAEA,MAAA,UAAA,EAAA,UAFA;AAGA,MAAA,eAAA,EAAA,eAHA;AAIA,MAAA,kBAAA,EAAA,kBAJA;AAKA,MAAA,gBAAA,EAAA,KALA;AAMA,MAAA,KAAA,EAAA,UANA;AAOA,MAAA,UAAA,EAAA;AAPA,KAAA;AASA,GAbA;AAcA,EAAA,OAAA,EAAA;AACA,IAAA,cADA,0BACA,QADA,EACA;AACA,UAAA,QAAA,KAAA,KAAA,EAAA;AACA,aAAA,KAAA,GAAA,UAAA;AACA,OAFA,MAEA,IAAA,QAAA,KAAA,UAAA,EAAA;AACA,aAAA,KAAA,GAAA,kBAAA;AACA,OAFA,MAEA;AACA,aAAA,KAAA,GAAA,aAAA;AACA;;AACA,WAAA,gBAAA,GAAA,QAAA;AACA;AAVA;AAdA,CAAA","sourcesContent":["<template>\n  <div class=\"home\">\n    <ApolloQuery :query=\"categoriesQuery\">\n      <template slot-scope=\"{ result: { data, loading }, isLoading }\">\n        <div v-if=\"isLoading\">Loading...</div>\n        <ul v-else class=\"list-reset text-lg\">\n          <li class=\"mb-6\">\n            <a\n              href=\"#\"\n              class=\"text-black hover:text-grey-darkest\"\n              @click.prevent=\"selectCategory('all')\"\n              >All</a\n            >\n          </li>\n          <li class=\"mb-6\">\n            <a\n              href=\"#\"\n              class=\"text-black hover:text-grey-darkest\"\n              @click.prevent=\"selectCategory('featured')\"\n              >Featured</a\n            >\n          </li>\n          <li\n            v-for=\"category of data.categories\"\n            :key=\"category.id\"\n            class=\"mb-6\"\n          >\n            <a\n              href=\"#\"\n              class=\"text-black hover:text-grey-darkest\"\n              @click.prevent=\"selectCategory(category.id)\"\n              >{{ category.name }}</a\n            >\n          </li>\n          <li class=\"mb-6\">\n            <router-link\n              to=\"/books/add\"\n              class=\"text-black hover:text-grey-darkest\"\n              >Add a book</router-link\n            >\n          </li>\n        </ul>\n        <!-- <div>\n          <a\n            href=\"#\"\n            v-for=\"category of data.categories\"\n            :key=\"category.id\"\n            @click=\"selectCategory(category.id)\"\n            class=\"link-margin\"\n            >{{ category.id }} {{ category.name }}</a\n          >\n        </div> -->\n      </template>\n    </ApolloQuery>\n    <div v-if=\"selectedCategory == 'all'\">\n      <ApolloQuery :query=\"query\">\n        <template slot-scope=\"{ result: { data, loading }, isLoading }\">\n          <div v-if=\"isLoading\">Loading...</div>\n          <div v-else>\n            <div v-for=\"book of data.books\" :key=\"book.id\">\n              <router-link :to=\"`/books/${book.id}`\">\n                {{ book.id }}. {{ book.title }}\n              </router-link>\n            </div>\n          </div>\n        </template>\n      </ApolloQuery>\n    </div>\n    <div v-else-if=\"selectedCategory == 'featured'\">\n      <ApolloQuery :query=\"query\" :variables=\"{ featured: true }\">\n        <template slot-scope=\"{ result: { data, loading }, isLoading }\">\n          <div v-if=\"isLoading\">Loading...</div>\n          <div v-else>\n            <div v-for=\"book of data.booksByFeatured\" :key=\"book.id\">\n              {{ book.id }} {{ book.title }}\n            </div>\n          </div>\n        </template>\n      </ApolloQuery>\n    </div>\n    <ApolloQuery :query=\"query\" :variables=\"{ id: selectedCategory }\">\n      <template slot-scope=\"{ result: { data, loading }, isLoading }\">\n        <div v-if=\"isLoading\">Loading...</div>\n        <div v-else>\n          <div v-for=\"book of data.category.books\" :key=\"book.id\">\n            {{ book.id }} {{ book.title }}\n          </div>\n        </div>\n      </template>\n    </ApolloQuery>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\nimport categoryQuery from \"../graphql/queries/Category.gql\";\nimport categoriesQuery from \"../graphql/queries/Categories.gql\";\nimport booksQuery from \"../graphql/queries/Books.gql\";\nimport booksFeaturedQuery from \"../graphql/queries/BooksFeatured.gql\";\n// eslint-disable-next-line no-unused-vars\n\nexport default {\n  name: \"home\",\n  components: {},\n  data() {\n    return {\n      categoryQuery,\n      booksQuery,\n      categoriesQuery,\n      booksFeaturedQuery,\n      selectedCategory: \"all\",\n      query: booksQuery,\n      categories: []\n    };\n  },\n  methods: {\n    selectCategory(category) {\n      if (category === \"all\") {\n        this.query = booksQuery;\n      } else if (category === \"featured\") {\n        this.query = booksFeaturedQuery;\n      } else {\n        this.query = categoryQuery;\n      }\n      this.selectedCategory = category;\n    }\n  }\n};\n</script>\n\n<style>\n.link-margin {\n  margin-left: 24px;\n}\n</style>\n"],"sourceRoot":"src/views"}]}